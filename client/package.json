    "preview": "npx serve dist"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2"
  },
  "devDependencies": {
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "concurrently": "^9.2.1",
    "esbuild": "^0.25.9",
    "live-server": "^1.2.2",
    "typescript": "^5.9.2"
  }
}
